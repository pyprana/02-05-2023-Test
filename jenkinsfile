// Jenkins file
pipeline
{
    stages {

        steps {
            
            echo "This is for double quotes"
        
        }
    
    stages {

        steps {

            echo 'This is for single quotes'
        }
    }

    stages {

        steps {

            sh 'this is single quotes'
        }
    }

    stages {

        steps {

            sh "This is double quotes"
            
        }
    }
    
    }

}
